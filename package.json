{
  "name": "spotify-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "browserify": "browserify components/app-browser.js -o public/javascripts/app.js",
    "watchify": "watchify components/app-browser.js -o public/javascripts/app.js",
    "debug": "npm run watchify & node debug ./bin/www",
    "start": "npm run watchify & node ./bin/www",
    "test": "istanbul cover _mocha -- --compilers js:babel/register --recursive",
    "posttest": "istanbul check-coverage"
  },
  "dependencies": {
    "body-parser": "~1.12.4",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.12.4",
    "jade": "~1.9.2",
    "jquery": "^2.1.4",
    "node-jsx": "^0.13.3",
    "react": "^0.13.3",
    "request": "^2.58.0"
  },
  "devDependencies": {
    "babel": "^5.5.8",
    "browserify": "^10.2.4",
    "istanbul": "^0.3.15",
    "jsdom": "^3.1.2",
    "mocha": "^2.2.5",
    "react-tools": "^0.13.3",
    "reactify": "^1.1.1",
    "watchify": "^3.2.2"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
